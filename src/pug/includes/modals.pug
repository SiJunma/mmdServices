        | Apply for a Job
      
      button.btn-close(aria-label="Close modal", type="button")

    .modal-body
      .apply-form
        form(action="")
          .form_input
            input.input-form#jobApply_applyerName(required, type="text", name="Applyer Name", placeholder="Name") 

          .btn-group
            .form_input
              input.input-form#jobApply_applyerMail(required, type="text", name="Applyer Mail", placeholder="E-mail")
            .form_input
              input.input-form#jobApply_applyerPhone(required, type="tel", name="Applyer Phone", placeholder="Phone number")

          .btn-file
            label(for='jobApply_applyerResume').btn.input-file-btn Upload Resume
            div.btn-file__siblings
              span.input-file-box No file chosen
              input.input-form.input-file-actual#jobApply_applyerResume(required, hidden type="file", name="Applyer Resume") 
        
          .form_input
            textarea.input-textarea#jobApply_applyerMsg(name="Applyer Letter", placeholder="Accompanying letter")

          .form_input
            input.input-form#jobApply_jobTitle(required, hidden  type="text", name="Job Title", value="link on job page!!!!!") 

          .form_input
            textarea.input-textarea#jobApply_applyerLinks(name="Applyer links", placeholder="Links to LinkedIn, GitHub, etc.")

          .form_input
            input.input-form#jobApply_applyerReff(type="text", name="Refferal", placeholder="Were you reffered by someone?") 

          button.btn(type="submit") Submit
          